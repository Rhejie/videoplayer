{
  "name": "videoplayer",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vimeo/player": "^2.23.0",
    "video.js": "^8.12.0",
    "videojs-youtube": "^3.0.1",
    "vite-plugin-dts": "^3.9.1",
    "vue": "^3.4.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webinarinc/VideoPlayer"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/revdojo-video-player-lib.js",
  "main": "./dist/revdojo-video-player-lib.umd.cjs",
  "exports": {
    ".": {
      "import": "./dist/revdojo-video-player-lib.js",
      "require": "./dist/revdojo-video-player-lib.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "./dist/main.d.ts",
  "keywords": [
    "revdojo",
    "video-player",
    "video.js",
    "videojs-youtube",
    "vimeo",
    "vue3",
    "vue"
  ],
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.5",
    "@types/video.js": "^7.3.58",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vue-tsc": "^2.0.11"
  }
}
